@{
}

@model IEnumerable<string>
<div class="list-group">
    <a class="btn btn-outline-secondary mb-2" asp-controller="Home" asp-action="Index" asp-route-bookType="" asp-route-pageNum="1">Home</a>

        @foreach (string c in Model)
        {
                <a class="btn @(ViewBag.SelectedType == c ? "btn-primary":"btn-outline-secondary")  mb-2"  asp-controller="Home" asp-action="Index" asp-route-bookType="@c" asp-route-pageNum="1">@c</a>
        }
</div>